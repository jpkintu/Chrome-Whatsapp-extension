{
    "manifest_version": 3,
    "name": "WhatsApp Link Creator",
    "version": "1.1",
    "description": "Automatically create a WhatsApp link for a hovered phone number",
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "whatsapp-icon.png",
        "48": "whatsapp-icon.png",
        "128": "whatsapp-icon.png"
    }
}
